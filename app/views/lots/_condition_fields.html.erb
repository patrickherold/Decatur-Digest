<div class="field">
  <%= f.attribute_fields do |a| %>
    <%= a.attribute_select :selected => "Owner" %>
  <% end %>
  <%= f.predicate_select only: [:cont, :not_cont, :eq, :not_eq, :lt ]%>
  <%= f.value_fields do |v| %>
    <%= v.text_field :value, :placeholder => "Enter search term"%>
  <% end %>
  <%= link_to "remove", '#', class: "remove_fields btn btn-danger btn-mini" %>
</div>




